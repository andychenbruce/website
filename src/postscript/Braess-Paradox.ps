%
%  PWM-Circuit.ps
%

/gs {gsave} def
/gr {grestore} def

/np {newpath} def
/cp {closepath} def
/m2 {moveto} def
/l2 {lineto} def
/r2 {rlineto} def
/tr {translate} def
/sk {stroke} def
/slw {setlinewidth} def

/dot {0 360 arc fill} def %% x y r circle

/Helvetica-Bold findfont 12 scalefont setfont

0 300 tr

0.9 0.9 scale

/Resistor {
    gs
    tr
    np
    
    gs %% Whiteout the line
      4 slw
      1 1 1 setrgbcolor
      0 0 m2
      0 36 l2
      sk
    gr
    
    np
    0  0 m2
    5  3 l2
   -5  9 l2
    5 15 l2
   -5 21 l2
    5 27 l2
   -5 33 l2
    0 36 l2
    sk
    /Helvetica-Bold findfont 10 scalefont setfont
    16 18 m2 (4) show
     6  8 m2 (ohms) show
    gr
} def

/LED {
    gs
    tr
    np
    -10 0 m2
    10  0 l2
    
     0  14 l2
    10  14 l2
    13  20 l2
    10  20 l2
     8  17 l2
    -10 17 l2
    -13 11 l2
    -10 11 l2
     -8 14 l2
      0 14 l2
    cp
    fill
    /Helvetica-Bold findfont 10 scalefont setfont
    13 6 m2 (5v) show
    13 -5 m2 (Zener) show
    gr
} def

/TopY 245 def
/BotY  95 def
/DotX 240 def
/LftX 140 def
/RgtX 201 def
/MidY 165 def

gs
2 slw
1 0 0 setrgbcolor
np
LftX MidY m2
LftX 20 add MidY l2
LftX 40 add MidY 10 add l2
sk
np
LftX 40 add MidY m2
RgtX MidY l2
sk
gr

0 0 .8 setrgbcolor
210 230 moveto (+8 Volts) show
DotX TopY 5 dot
DotX TopY m2
LftX TopY l2
sk

%% 110 245 m2
%% 201 245 l2
%% 201 174 l2
%% sk

np
LftX TopY m2
LftX BotY l2
sk
np
RgtX TopY m2
RgtX BotY l2
sk

np
LftX BotY m2
DotX BotY l2
sk
LftX 110 Resistor
LftX 200 LED


0 0 1 setrgbcolor
208 82 moveto (GND) show
DotX  95 5 dot
RgtX 185 Resistor
RgtX 120 LED
 
showpage
